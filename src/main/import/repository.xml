<?xml version="1.0" encoding="UTF-8"?>
<content xmlns:j="http://www.jahia.org/jahia/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0">
   <modules jcr:primaryType="jnt:modules">
      <industrial j:dependencies="animate bootstrap4-components bootstrap4-core codemirror-editor default jquery popperjs se-utils"
                  j:installedModules="industrial"
                  j:modulePriority="0"
                  j:moduleType="templatesSet"
                  j:title="industrial"
                  jcr:mixinTypes="jmix:hasExternalProviderExtension"
                  jcr:primaryType="jnt:module">
         <portlets jcr:primaryType="jnt:portletFolder"/>
         <files jcr:primaryType="jnt:folder"/>
         <contents jcr:primaryType="jnt:contentFolder"/>
         <templates jcr:primaryType="jnt:templatesFolder">
            <files jcr:primaryType="jnt:folder">
               <themes jcr:primaryType="jnt:folder">
                  <default jcr:primaryType="jnt:folder"/>
               </themes>
            </files>
            <contents jcr:primaryType="jnt:contentFolder"/>
            <base j:view="industrial"
                  jcr:mixinTypes="jmix:theme"
                  jcr:primaryType="jnt:template">
               <footercontent jcr:primaryType="jnt:contentList">
                  <footer-static-area j:limitedAbsoluteAreaEdit="false" jcr:primaryType="jnt:absoluteArea"/>
               </footercontent>
               <headercontent jcr:primaryType="jnt:contentList">
                  <main-navigation jcr:mixinTypes="bootstrap4mix:createSection"
                                   jcr:primaryType="bootstrap4nt:grid"
                                   sectionElement="header"
                                   sectionRole="banner">
                     <main-navigation jcr:primaryType="jnt:contentList">
                        <header-static-area j:limitedAbsoluteAreaEdit="false" jcr:primaryType="jnt:absoluteArea"/>
                     </main-navigation>
                  </main-navigation>
               </headercontent>
               <free jcr:primaryType="jnt:pageTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Free"/>
                  <headercontent jcr:primaryType="jnt:contentList"/>
               </free>
               <structured-template j:hiddenTemplate="false" jcr:primaryType="jnt:pageTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Structured Template"/>
                  <pagecontent jcr:primaryType="jnt:contentList">
                     <page-heading j:allowedTypes="tint:heading"
                                   j:areaView="default"
                                   j:numberOfItems="1"
                                   jcr:primaryType="jnt:area"/>
                     <article-grid gridClasses="col-lg-7 order-md-2,col-md-5 pr-md-5 mb-5"
                                   gridLayout="fixed-width"
                                   jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:createSection bootstrap4mix:createContainer bootstrap4mix:customGrid"
                                   jcr:primaryType="bootstrap4nt:grid"
                                   rowCssClass="align-items-center"
                                   rowHorizontalAlignment="default"
                                   rowVerticalAlignment="default"
                                   sectionCssClass="section"
                                   sectionElement="section"
                                   typeOfGrid="customGrid">
                        <article-grid-col1 jcr:primaryType="jnt:contentList">
                           <article-content-area j:allowedTypes="tint:text"
                                                 j:numberOfItems="1"
                                                 jcr:primaryType="jnt:area"/>
                        </article-grid-col1>
                        <article-grid-col0 jcr:primaryType="jnt:contentList">
                           <article-image-area j:allowedTypes="tint:image"
                                               j:numberOfItems="1"
                                               jcr:primaryType="jnt:area"/>
                        </article-grid-col0>
                     </article-grid>
                     <media-blocks-section gridLayout="fixed-width"
                                           jcr:mixinTypes="bootstrap4mix:createSection bootstrap4mix:createContainer"
                                           jcr:primaryType="bootstrap4nt:grid"
                                           sectionCssClass="section bg-light"
                                           sectionElement="section">
                        <media-blocks-section jcr:mixinTypes="jmix:systemNameReadonly"
                                              jcr:primaryType="jnt:contentList">
                           <title-row gridClasses="col-12 text-center"
                                      jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:customGrid"
                                      jcr:primaryType="bootstrap4nt:grid"
                                      rowCssClass="mb-5"
                                      rowHorizontalAlignment="default"
                                      rowVerticalAlignment="default"
                                      typeOfGrid="customGrid">
                              <title-row-col0 jcr:mixinTypes="jmix:systemNameReadonly"
                                              jcr:primaryType="jnt:contentList">
                                 <title-media-area j:allowedTypes="tint:text"
                                                   j:numberOfItems="1"
                                                   jcr:primaryType="jnt:area"/>
                              </title-row-col0>
                           </title-row>
                           <media-blocks-row gridClasses="col-md-6 col-lg-4,col-md-6 col-lg-4,col-md-6 col-lg-4"
                                             jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:customGrid"
                                             jcr:primaryType="bootstrap4nt:grid"
                                             rowHorizontalAlignment="default"
                                             rowVerticalAlignment="default"
                                             typeOfGrid="customGrid">
                              <content-row-col1 jcr:primaryType="jnt:contentList">
                                 <area-content-1 j:allowedTypes="tint:text"
                                                 j:numberOfItems="2"
                                                 jcr:primaryType="jnt:area"/>
                              </content-row-col1>
                              <content-row-col2 jcr:primaryType="jnt:contentList">
                                 <area-content-2 j:allowedTypes="tint:text"
                                                 j:numberOfItems="2"
                                                 jcr:primaryType="jnt:area"/>
                              </content-row-col2>
                              <content-row-col0 jcr:primaryType="jnt:contentList">
                                 <area-figure-1 j:allowedTypes="bootstrap4nt:figure"
                                                j:numberOfItems="1"
                                                j:subNodesView="industrial.features"
                                                jcr:primaryType="jnt:area"/>
                              </content-row-col0>
                              <media-blocks-row-col0 jcr:mixinTypes="jmix:systemNameReadonly"
                                                     jcr:primaryType="jnt:contentList">
                                 <media-block-1-area j:allowedTypes="tint:text"
                                                     j:numberOfItems="1"
                                                     jcr:primaryType="jnt:area"/>
                              </media-blocks-row-col0>
                              <media-blocks-row-col1 jcr:mixinTypes="jmix:systemNameReadonly"
                                                     jcr:primaryType="jnt:contentList">
                                 <media-block-2-area j:allowedTypes="tint:text"
                                                     j:numberOfItems="1"
                                                     jcr:primaryType="jnt:area"/>
                              </media-blocks-row-col1>
                              <media-blocks-row-col2 jcr:mixinTypes="jmix:systemNameReadonly"
                                                     jcr:primaryType="jnt:contentList">
                                 <media-block-3-area j:allowedTypes="tint:text"
                                                     j:numberOfItems="1"
                                                     jcr:primaryType="jnt:area"/>
                              </media-blocks-row-col2>
                           </media-blocks-row>
                        </media-blocks-section>
                     </media-blocks-section>
                     <card-section gridLayout="fixed-width"
                                   jcr:mixinTypes="bootstrap4mix:createSection bootstrap4mix:createContainer"
                                   jcr:primaryType="bootstrap4nt:grid"
                                   sectionCssClass="section"
                                   sectionElement="section">
                        <card-section jcr:mixinTypes="jmix:systemNameReadonly"
                                      jcr:primaryType="jnt:contentList">
                           <title-row gridClasses="col-md-8 text-center"
                                      jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:customGrid"
                                      jcr:primaryType="bootstrap4nt:grid"
                                      rowCssClass="justify-content-center mb-5"
                                      rowHorizontalAlignment="default"
                                      rowVerticalAlignment="default"
                                      typeOfGrid="customGrid">
                              <title-row-col0 jcr:mixinTypes="jmix:systemNameReadonly"
                                              jcr:primaryType="jnt:contentList">
                                 <title-card-area j:allowedTypes="tint:text"
                                                  j:numberOfItems="1"
                                                  jcr:primaryType="jnt:area"/>
                              </title-row-col0>
                           </title-row>
                           <card-row grid="3_3_3_3"
                                     jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:predefinedGrid"
                                     jcr:primaryType="bootstrap4nt:grid"
                                     rowHorizontalAlignment="default"
                                     rowVerticalAlignment="default"
                                     typeOfGrid="predefinedGrid">
                              <card-row-main jcr:mixinTypes="jmix:systemNameReadonly"
                                             jcr:primaryType="jnt:contentList">
                                 <card-1-area j:allowedTypes="tint:card"
                                              j:numberOfItems="1"
                                              jcr:primaryType="jnt:area"/>
                              </card-row-main>
                              <card-row-side jcr:mixinTypes="jmix:systemNameReadonly"
                                             jcr:primaryType="jnt:contentList">
                                 <card-2-area j:allowedTypes="tint:card"
                                              j:numberOfItems="1"
                                              jcr:primaryType="jnt:area"/>
                              </card-row-side>
                              <card-row-extra jcr:mixinTypes="jmix:systemNameReadonly"
                                              jcr:primaryType="jnt:contentList">
                                 <card-3-area j:allowedTypes="tint:card"
                                              j:numberOfItems="1"
                                              jcr:primaryType="jnt:area"/>
                              </card-row-extra>
                              <card-row-extra2 jcr:mixinTypes="jmix:systemNameReadonly"
                                               jcr:primaryType="jnt:contentList">
                                 <card-4-area j:allowedTypes="tint:card"
                                              j:numberOfItems="1"
                                              jcr:primaryType="jnt:area"/>
                              </card-row-extra2>
                           </card-row>
                        </card-section>
                     </card-section>
                     <gallery-section jcr:mixinTypes="bootstrap4mix:createSection"
                                      jcr:primaryType="bootstrap4nt:grid"
                                      sectionCssClass="section bg-light border-t pb-0"
                                      sectionElement="section">
                        <gallery-section jcr:mixinTypes="jmix:systemNameReadonly"
                                         jcr:primaryType="jnt:contentList">
                           <content-row gridClasses="col-md-8 text-center"
                                        gridLayout="fixed-width"
                                        jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:createContainer bootstrap4mix:customGrid"
                                        jcr:primaryType="bootstrap4nt:grid"
                                        rowCssClass="justify-content-center mb-5"
                                        rowHorizontalAlignment="default"
                                        rowVerticalAlignment="default"
                                        typeOfGrid="customGrid">
                              <content-row-col0 jcr:mixinTypes="jmix:systemNameReadonly"
                                                jcr:primaryType="jnt:contentList">
                                 <title-gallery-area j:allowedTypes="tint:text"
                                                     j:numberOfItems="1"
                                                     jcr:primaryType="jnt:area"/>
                              </content-row-col0>
                           </content-row>
                           <container-gallery gridClasses="col-md-4,col-md-4,col-md-4"
                                              gridLayout="full-width"
                                              jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:createContainer bootstrap4mix:customGrid"
                                              jcr:primaryType="bootstrap4nt:grid"
                                              rowCssClass="no-gutters"
                                              rowHorizontalAlignment="default"
                                              rowVerticalAlignment="default"
                                              typeOfGrid="customGrid">
                              <container-gallery-col0 jcr:mixinTypes="jmix:systemNameReadonly"
                                                      jcr:primaryType="jnt:contentList">
                                 <gallery-image-1-area j:allowedTypes="tint:galleryImage"
                                                       j:numberOfItems="1"
                                                       jcr:primaryType="jnt:area"/>
                              </container-gallery-col0>
                              <container-gallery-col1 jcr:mixinTypes="jmix:systemNameReadonly"
                                                      jcr:primaryType="jnt:contentList">
                                 <gallery-image-2-area j:allowedTypes="tint:galleryImage"
                                                       j:numberOfItems="1"
                                                       jcr:primaryType="jnt:area"/>
                              </container-gallery-col1>
                              <container-gallery-col2 jcr:mixinTypes="jmix:systemNameReadonly"
                                                      jcr:primaryType="jnt:contentList">
                                 <gallery-image-3-area j:allowedTypes="tint:galleryImage"
                                                       j:numberOfItems="1"
                                                       jcr:primaryType="jnt:area"/>
                              </container-gallery-col2>
                           </container-gallery>
                        </gallery-section>
                     </gallery-section>
                     <call-to-action gridClasses="col-lg-8,col-lg-4 text-lg-right"
                                     gridLayout="fixed-width"
                                     jcr:mixinTypes="bootstrap4mix:createRow bootstrap4mix:createSection bootstrap4mix:createContainer bootstrap4mix:customGrid"
                                     jcr:primaryType="bootstrap4nt:grid"
                                     rowCssClass="align-items-center"
                                     rowHorizontalAlignment="default"
                                     rowVerticalAlignment="default"
                                     sectionCssClass="section bg-primary"
                                     sectionElement="section"
                                     typeOfGrid="customGrid">
                        <call-to-action-col0 jcr:primaryType="jnt:contentList">
                           <call-2-action-content-area j:allowedTypes="tint:text"
                                                       j:numberOfItems="1"
                                                       jcr:primaryType="jnt:area"/>
                        </call-to-action-col0>
                        <call-to-action-col1 jcr:primaryType="jnt:contentList">
                           <call-2-action-btn-area j:allowedTypes="tint:text"
                                                   j:numberOfItems="1"
                                                   jcr:primaryType="jnt:area"/>
                        </call-to-action-col1>
                     </call-to-action>
                     <extra-area j:allowedTypes="bootstrap4nt:grid" jcr:primaryType="jnt:area"/>
                  </pagecontent>
                  <headercontent jcr:primaryType="jnt:contentList"/>
                  <footercontent jcr:primaryType="jnt:contentList"/>
                  <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Gabarit Structuré"/>
                  <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Structured Template"/>
                  <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Structured Template"/>
                  <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Structured Template"/>
                  <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="Structured Template"/>
                  <j:translation_de_DE jcr:language="de_DE"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="Structured Template"/>
               </structured-template>
               <industrial-content-template j:applyOn="jmix:mainResource"
                                             j:defaultTemplate="true"
                                             j:hiddenTemplate="false"
                                             jcr:primaryType="jnt:contentTemplate">
                  <j:translation_en jcr:language="en"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_de jcr:language="de"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_pt jcr:language="pt"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_it jcr:language="it"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_fr jcr:language="fr"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_es jcr:language="es"
                                    jcr:mixinTypes="mix:title"
                                    jcr:primaryType="jnt:translation"
                                    jcr:title="industrial content template"/>
                  <j:translation_de_DE jcr:language="de_DE"
                                       jcr:mixinTypes="mix:title"
                                       jcr:primaryType="jnt:translation"
                                       jcr:title="industrial content template"/>
                  <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                               jcr:primaryType="jnt:contentList">
                     <main-resource-display j:mainResourceView="hidden.mainResource" jcr:primaryType="jnt:mainResourceDisplay"/>
                  </pagecontent>
               </industrial-content-template>
            </base>
            <rss j:applyOn="tint:lastContent"
                 j:defaultTemplate="true"
                 j:hiddenTemplate="false"
                 jcr:primaryType="jnt:contentTemplate">
               <j:translation_en jcr:language="en" jcr:primaryType="jnt:translation" jcr:title="rss"/>
               <pagecontent jcr:mixinTypes="jmix:systemNameReadonly"
                            jcr:primaryType="jnt:contentList">
                  <main-resource-display j:mainResourceView="default" jcr:primaryType="jnt:mainResourceDisplay"/>
               </pagecontent>
            </rss>
         </templates>
         <home j:isHomePage="true"
               j:templateName="free"
               jcr:primaryType="jnt:page">
            <j:translation_en jcr:language="en"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_de jcr:language="de"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_fr_BE jcr:language="fr_BE"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Home"/>
            <j:translation_pt jcr:language="pt"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_it jcr:language="it"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_fr jcr:language="fr"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_da jcr:language="da"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_es jcr:language="es"
                              jcr:mixinTypes="mix:title"
                              jcr:primaryType="jnt:translation"
                              jcr:title="Home"/>
            <j:translation_de_DE jcr:language="de_DE"
                                 jcr:mixinTypes="mix:title"
                                 jcr:primaryType="jnt:translation"
                                 jcr:title="Home"/>
         </home>
      </industrial>
   </modules>
</content>
